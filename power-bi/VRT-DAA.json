{
    "name": "VRT DAA Theme",
    "dataColors": [
        "#031037",
        "#AA3CE9",
        "#5541F0",
        "#fc4662",
        "#3bc58f",
        "#FFBE32",
        "#0081FF",
        "#00BEAA",
        "#FF3C78",
        "#3599B8"
    ],
    "bad": "#FF4944",
    "neutral": "#FFBE32",
    "good": "#3ECF6E",
    "minimum": "#0081FF",
    "center": "#FFBE32",
    "maximum": "#5541F0",
    "background":"#FFFFFF",
    "secondaryBackground": "#FFFFFF",
    "foreground": "#031037",
    "tableAccent": "#031037",
    "firstLevelElements": "#031037",
    "secondLevelElements": "#031037",
    "thirdLevelElements": "#031037",
    "fourthLevelElements": "#031037",
    "textClasses": {
        "label": {
            "fontSize": 11,
            "fontFace": "'Roobert VRT','Roobert',arial, sans-serif",
            "color": "#031037"
        },
        "title": {
            "fontSize": 11,
            "fontFace": "'Roobert Semibold','Roobert VRT','Roobert', arial, sans-serif",
            "color": "#031037"
        },
        "header": {
            "fontFace": "'Roobert Semibold','Roobert VRT','Roobert', arial, sans-serif",
            "fontSize": 20,
            "color": "#031037"
        },
        "callout": {
            "fontSize": 30,
            "fontFace": "'Roobert medium','Roobert VRT','Roobert', arial, sans-serif",
            "color": "#031037"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "fontSize": 11,
                        "fontFamily": "'Roobert VRT','Roobert', arial, sans-serif",
                        "color": "#031037"
                    }
                ]
            }
        },
        "*": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "show": false,
                        "radius": 4
                    }
                ],
                "visualHeader": [
                    {
                        "transparency": 0,
                        "background": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foreground": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "border": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ],
                "visualTooltip": [
                    {
                        "titleFontColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "valueFontColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "filterCard": [
                    {
                        "$id": "Applied",
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0,
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    },
                    {
                        "$id": "Available",
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspacePane": [
                    {
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ]
            }
        },
        "card": {
            "*": {
              "title": [
                {
                  "fontFamily": "'Roobert Semibold','Roobert VRT','Roobert'",
                  "alignment": "center",
                  "fontSize": 11
                }
              ],
              "labels": [
                {
                  "fontFamily": "'Roobert Semibold','Roobert VRT','Roobert'",
                    "color": {
                    "solid": {
                      "color": "#031037"
                    }
                  },
                  "fontSize": 30
                }
              ],
              "background": [
                {
                    "color": {
                        "solid": {
                            "color": "#f7f9fd"
                        }
                    },
                    "transparency": 0
                }
            ],
              "border": [
                {
                    "color": {
                        "solid": {
                            "color": "#f7f9fd"
                        }
                    },
                    "show": true,
                    "radius": 20
                }
            ]
            }
          },
        "textbox": {
            "*": {
                "*": [
                    {
                        "fontSize": 9,
                        "fontFamily": "'Roobert Semibold','Roobert VRT','Roobert'",
                        "wordWrap": true,
                        "color": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "background": [
                    {
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 100
                    }
                ],
                "visualHeader": [
                    {
                        "show": false
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ]
            }
        }
    }
}
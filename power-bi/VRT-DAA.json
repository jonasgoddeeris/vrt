    "textClasses": {
        "label": {
            "fontSize": 11,
            "fontFace": "'Roobert VRT','Roobert',wf_standard-font,arial"
        },
        "title": {
            "fontSize": 11,
            "fontFace": "'Roobert Semibold', arial"
        },
        "header": {
            "fontFace": "'Roobert Semibold', wf_standard-font, arial",
            "fontSize": 20
        },
        "callout": {
            "fontSize": 30,
            "fontFace": "'Roobert medium', wf_standard-font, arial"
        }
    },
    "visualStyles": {
        "*": {
            "*": {
                "*": [
                    {
                        "fontSize": 12,
                        "fontFamily": "'Roobert VRT','Roobert Semibold','Roobert'",
                        "color": "#031037",
                        "fontWeight": "SemiBold"
                    }
                ]
            }
        },
        "*": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "border": [
                    {
                        "color": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "show": false,
                        "radius": 4
                    }
                ],
                "visualHeader": [
                    {
                        "transparency": 0,
                        "background": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foreground": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "border": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ],
                "visualTooltip": [
                    {
                        "titleFontColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "valueFontColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "background": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ],
                "filterCard": [
                    {
                        "$id": "Applied",
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0,
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    },
                    {
                        "$id": "Available",
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspacePane": [
                    {
                        "backgroundColor": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "foregroundColor": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ]
            }
        },
        "card": {
            "*": {
              "dropShadow": [
                {
                  "shadowBlur": 16,
                  "angle": 12,
                  "shadowDistance": 4,
                  "preset": "BottomLeft",
                  "position": "Outer",
                  "show": true,
                  "shadowSpread": 16,
                  "transparency": 90
                }
              ],
              "title": [
                {
                  "fontFamily": "'Roobert Semibold','Roobert VRT','Roobert'",
                  "alignment": "center",
                  "fontSize": 12
                }
              ]
            }
          },
        "textbox": {
            "*": {
                "*": [
                    {
                        "fontSize": 20,
                        "fontFamily": "'Roobert Semibold','Roobert VRT','Roobert'",
                        "wordWrap": true,
                        "color": {
                            "solid": {
                                "color": "#031037"
                            }
                        }
                    }
                ],
                "general": [
                    {
                        "responsive": true
                    }
                ],
                "background": [
                    {
                        "show": false,
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 100
                    }
                ],
                "visualHeader": [
                    {
                        "show": false
                    }
                ]
            }
        },
        "page": {
            "*": {
                "background": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        },
                        "transparency": 0
                    }
                ],
                "outspace": [
                    {
                        "color": {
                            "solid": {
                                "color": "#FFFFFF"
                            }
                        }
                    }
                ]
            }
        }
    }
